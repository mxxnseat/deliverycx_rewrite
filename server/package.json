    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.2.5",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/websockets": "^8.2.5",
    "@typegoose/typegoose": "^9.2.0",
    "@types/axios": "^0.14.0",
    "@types/joi": "^17.2.3",
    "amqp-connection-manager": "^4.1.1",
    "amqplib": "^0.8.0",
    "axios": "^0.24.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.0.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express-session": "^1.17.2",
    "is-in-subnet": "^4.0.1",
    "joi": "^17.5.0",
    "nestjs-pino": "^2.4.0",
    "nodemailer": "^6.7.2",
    "pino": "^7.6.2",
    "pino-http": "^6.5.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^7.3.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.4.1",
    "swagger-ui-express": "^4.2.0",
    "uuid": "^8.3.2"
  },
